<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="mainMenu" class="com.medigy.app.pbs.menu.BasicMenu">
        <constructor-arg index="0"><value>Main Menu</value></constructor-arg>
        <constructor-arg index="1"><value>false</value></constructor-arg>
        <property name="menuItemLabels">
            <list>
                <value>Home</value>
                <value>TestPage1</value>
                <value>TestPage2</value>
                <value>TestPage3</value>
                <value>Register Patient</value>
                <value>Edit Patient</value>
            </list>
        </property>
        <property name="menuItemClasses">
            <list>
                <value>com.medigy.app.pbs.page.HomePage</value>
                <value>com.medigy.app.pbs.page.TestPage1</value>
                <value>com.medigy.app.pbs.page.TestPage2</value>
                <value>com.medigy.app.pbs.page.TestPage3</value>
                <value>com.medigy.app.pbs.page.PatientRegistrationPage</value>
                <value>com.medigy.app.pbs.page.EditPatientPage</value>
            </list>
        </property>
    </bean>
</beans>